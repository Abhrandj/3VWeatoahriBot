modules = ["python-3.11"]
[interpreter]
language = "python3"

[env]
FLASK_ENV = "development"
FLASK_APP = "main.py"
PORT = "5000"

[run]
command = "python3 main.py"

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[nix]
channel = "stable-24_05"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[[ports]]
localPort = 5000
externalPort = 80
